includes:
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon

parameters:
    # PHP 8.3
    phpVersion: 80300

    treatPhpDocTypesAsCertain: false
    level: 10

    paths:
        - src
        - cli

services:
    -
        class: Phparch\SpaceTraders\PHPStan\ControllerMethodSignatureRule
        tags:
            - phpstan.rules.rule